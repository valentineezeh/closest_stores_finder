{
  "name": "jumbotest",
  "version": "1.0.0",
  "description": "Jumbo technical test API",
  "main": "index.js",
  "scripts": {
    "start": "node dist/bin/www.js",
    "clean:dist": "rimraf ./dist",
    "prebuild": "npm run clean:dist",
    "build:prod": "npm run prebuild && babel ./src --out-dir dist --copy-files",
    "heroku-postbuild": "npm run build:prod",
    "dev": "nodemon --watch src --delay 250ms --exec babel-node src/bin/www.js --ignore tests/",
    "bulkInsert": "node src/utils/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "errorhandler": "1.5.1",
    "express": "4.16.4",
    "express-validator": "5.3.1",
    "geolib": "3.3.1",
    "helmet": "3.18.0",
    "mongodb": "4.1.3",
    "morgan": "1.9.1",
    "nodemon": "1.18.3",
    "winston": "3.3.3"
  },
  "engines": {
    "node": "14.15.4",
    "npm": "6.14.10"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "cross-env": "5.2.0",
    "eslint": "5.15.2",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valentineezeh/closest_stores_finder.git"
  },
  "author": "Valentine",
  "license": "ISC"
}
